{
	"$schema": "https://turborepo.org/schema.json",
	"globalDependencies": [".env"],
	"globalEnv": ["PORT", "BASE_URL"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
		"start": {
			"dependsOn": ["^build"],
			"outputs": []
		},
		"release": {
			"dependsOn": ["^release"],
			"outputs": []
		},
    "test": {
      "outputs": []
    },
    "dev": {
      "cache": true
    }
  }
}
